<Window x:Class="prac13.AuthenticationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac13"
        mc:Ignorable="d"
        Title="AuthenticationWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnLogin" Click="btnLogin_Click" Content="Подтвердить" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBox x:Name="tbPassword" HorizontalAlignment="Center" Margin="0,170,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
