<Window x:Class="prac13.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac13"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="363" Width="673">
    <Grid>
        <TextBox x:Name="tbRows" HorizontalAlignment="Left" Margin="185,124,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbColumns" HorizontalAlignment="Left" Margin="383,124,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Save" Click="btnSaveSettings_Click" Content="Сохранить нстройки" HorizontalAlignment="Center" Margin="0,174,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
